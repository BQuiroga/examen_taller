

<div id="page-content-wrapper">
<p id="notice"><%= notice %></p>



 <div class="col-sm-9 col-sm-offset-3 col-md-10 col-md-offset-2 main">
          <h1 class="page-header">Listing Sessions</h1>

          <div class="row placeholders">
            <div class="col-xs-6 col-sm-3 placeholder">
             <img class="img-responsive" src="assets/ciclismo.jpg" alt="Generic placeholder image" >

              <h4>Cycling</h4>
              <span class="text-muted">Something else</span>
            </div>
            <div class="col-xs-6 col-sm-3 placeholder">
              <img class="img-responsive" src="assets/pesasindex.jpg" alt="Generic placeholder image">
              <h4>Weight Lifting</h4>
              <span class="text-muted">Something else</span>
            </div>
            <div class="col-xs-6 col-sm-3 placeholder">
              <img class="img-responsive" src="assets/trote.jpg" alt="Generic placeholder image" width="200" height="200">
              <h4>Jogging</h4>
              <span class="text-muted">Something else</span>
            </div>
          </div>
<div class="table-responsive" style="margin-left=-15px">
  <table class="table table-striped">
  <thead>
    <tr>
      
      <th>Type session</th>
      <th>Burned calories</th>
      <th>Hearth rate</th>
      <th>Total time</th>
      <th>Time started</th>
      <th>Time finished</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @sessions.each do |session| %>
      <tr>
        
        <td><%= session.type_session_id %></td>
        <td><%= session.burned_calories %></td>
        <td><%= session.hearth_rate %></td>
        <td><%= session.total_time %></td>
        <td><%= session.time_started %></td>
        <td><%= session.time_finished %></td>
        <td><%= link_to 'Show', session %></td>
        <td><%= link_to 'Edit', edit_fitness_session_path(session) %></td>
        <td><%= link_to 'Destroy', session, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
</div>
</div>
<br>

<%= link_to 'New Session', new_fitness_session_path %>
