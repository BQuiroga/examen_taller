<title>Band Information Records | FiTrack</title>
<div id="page-content-wrapper">
<div class="container"> 
<h1 style="border-bottom: 1px solid #A6AAA9; color: #000000;">Band Information Records</h1>
<p id="notice"><%= notice %></p>

<table>
  <thead>
    <tr>
      <th>Steps</th>
      <th>Lat</th>
      <th>Long</th>
      <th>User</th>
      <th>Band</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @band_informations.each do |band_information| %>
      <tr>
        <td><%= band_information.steps %></td>
        <td><%= band_information.lat %></td>
        <td><%= band_information.long %></td>
        <td><%= band_information.user_id%></td>
        <td><%= band_information.band_id %></td>
        <td><%= link_to 'Show', band_information %></td>
        <td><%= link_to 'Edit', edit_band_information_path(band_information) %></td>
        <td><%= link_to 'Destroy', band_information, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Band information', new_band_information_path %>
</div>
</div>